# Environment variables for Burnout Risk Prediction Backend
# Created by Balaji Koneti

# Server Configuration
PORT=3000
NODE_ENV=development

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/burnout-risk-prediction

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRES_IN=24h

# CORS Configuration
CORS_ORIGIN=http://localhost:5173

# Rate Limiting Configuration
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging Configuration
LOG_LEVEL=debug

# ML Model Configuration
MODEL_VERSION=1.0.0
MODEL_CONFIDENCE_THRESHOLD=0.7

# ML Service Configuration
ML_SERVICE_URL=http://localhost:8001
ML_API_TIMEOUT=10000

# Email Configuration (for future use)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password

# External API Configuration (for future use)
GOOGLE_CALENDAR_API_KEY=your-google-api-key
OUTLOOK_API_KEY=your-outlook-api-key
